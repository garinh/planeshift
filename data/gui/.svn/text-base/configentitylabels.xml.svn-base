                </nodewidget>
                <attr name="type" value="color"/>
            </widget>
            <widget factory="pawsSeqTreeNode" name="Grouped with you">
                <nodewidget>
                    <widget factory="pawsTextBox" name="GroupColorLabel">
                        <frame x="0" y="0" width="250" height="15" border="no" />
